-- decompressing gfx
do
	local g=""
	for c in all(gfx) do
		g..=
			sub(tostr(ord(c),0x1),5,6)
	end
	gfx=g
end

anim = {}
show_con = true
state = 1

base_chip={
	[0]=100,300,800,2000,5000,
	11000,20000,35000,50000
}
cur_blind = 0

o_con = "play"
x_con = "discard"

o_last_t = -20
move_card = false

function _init()
	deck.init()
	deck.shuffle()
	deck.draw(8)
	hand.sort_r()
	
	cur.cont = hand
	
	jokers = card_cont:new(2,3,85)
	tarots = card_cont:new(92,3,34)
	played = card_cont:new(14,65,100)
	
	target = 600
	score = 0
	chip = 0
	mult = 0
	h_types = {types:empty()}
	scoring=false
	
	play = 18
	disc = 3
	
	ante = 1
	
	money = 0
	
	for x=16,16 do
	jokers:add(
		joker:new(c_js[x])
	)
	end
	--played:add(card:new(x,1))
end

function _update()
	cls(1)
	input()
	
	qsort(hand.sel)
	
	for a in all(anim) do
		if 
			costatus(a) != "dead" 
		then
			assert(coresume(a))
		else
			del(anim,a)
		end
	end
end

function _draw()
	played:draw()
	jokers:draw()
	tarots:draw()
	
	head_ui()
	if (state==0) blind_ui()
	if (state==1) round_ui()
	if (state==2) return
end

-- ui
function head_ui()
	local pre_x = 
		59-(#tostr(chip))*7
	
	rrect(23,30,36,12,0) -- chip
	spr(143,60,30) -- x
	rrect(68,30,36,8,1) -- mult
	
	-- chip
	num_print(
		chip,
		pre_x,30
	)
	-- mult
	num_print(
		mult,
		68,30
	)
	
	-- hand
	if not scoring then
		rrect(23,40,81,5,2)
		print_cen(h_types[1].name,41,7)
	end	
	
	-- play
	spr(175,0,30)
	num_print(play,7,30,12)
	-- disc
	spr(191,0,40)
	num_print(disc,7,40,8)
	
	-- ante
	num_print(ante,106,30,9)
	spr(159,113,30)
	num_print(8,120,30,9)
	
	-- money
	spr(207,106,39)
	print(money,115,41,10)
	
	if show_con and not move_card then
		local s = 
			"🅾️ "..o_con..
			" ❎ "..x_con
		print_cen(s,50,6)
	end
	
	if move_card then
