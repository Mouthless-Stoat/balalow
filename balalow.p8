-- deck and helper
deck = {}

function deck.draw(n)
	n = n or 1
	
	for i=1,n do
		local t = deck[1]
		deli(deck,1)
		hand:add(t)
	end
end

function deck.init()
	clear(deck)
	for s=1,4 do
		for r=2,14 do
			add(deck,card:new(r,s))
		end
	end
end

function deck.shuffle()
	for c=1,#deck*4 do
		local r = rnd(deck)
		add(deck,r)
		del(deck,r)
	end
end

-- helper

-- https://pico-8.fandom.com/wiki/qsort
function qsort(a,c,l,r)
	c,l,r=c or function(a,b) return a<b end,l or 1,r or #a
	if l<r then
		if c(a[r],a[l]) then
			a[l],a[r]=a[r],a[l]
		end
		local lp,k,rp,p,q=l+1,l+1,r-1,a[l],a[r]
		while k<=rp do
			local swaplp=c(a[k],p)
			if swaplp or c(a[k],q) then
			else
				while c(q,a[rp]) and k<rp do
					rp-=1
				end
				a[k],a[rp],swaplp=a[rp],a[k],c(a[rp],p)
				rp-=1
			end
			if swaplp then
				a[k],a[lp]=a[lp],a[k]
				lp+=1
			end
			k+=1
		end
		lp-=1
		rp+=1
		a[l],a[lp]=a[lp],a[l]
		a[r],a[rp]=a[rp],a[r]
		qsort(a,c,l,lp-1       )
		qsort(a,c,  lp+1,rp-1  )
		qsort(a,c,       rp+1,r)
	end
end

function have(t,v)
	return count(t,v) >= 1
end

function feq(t)
	local o = {}
	for i in all(t) do
		o[i] = (o[i] or 0) + 1
	end
	return o
end

function vals(t)
	local o = {}
	for k,v in pairs(t) do
		add(o,v)
	end
	return o
end

function keys(t)
	local o = {}
	for k,v in pairs(t) do
		add(o,k)
	end
	return o
end

function cons(t)
	if (#t!=5) return false
	qsort(t)
	for i = 2, #t do
		if t[i]-t[i-1]!=1 then
			return false
		end
	end
	return true
end

function clear(t)
	for i=1,#t do
		deli(t)
	end
end

function sleep(fr)
	for x=1,fr do
		flip()
	end
end

function pause(fr)
	while fr > 0 do
		fr -= 1
		yield()
	end
end

function pause_f(fr, f)
	for i=1,fr do
		f(i)
		yield()
	end
end

function num_print(n,x,y,c)
	c = c or 7
	local s = tostr(n)
	pal(7,c)
	for i=1,#s do
		d = tonum(s[i])
		spr(d,(i-1)*7+x,y)
	end
	pal()
end

-- mode
-- 0 left
-- 1 right
-- 2 both
function rrect(x,y,w,c,m)
	local l = (m==0 or m==2) and 8 or 0
	local r = (m==1 or m==2) and 8 or 0
	rectfill(x+l,y,x+w-r,y+7,c)
	
	pal(7,c)
	if (m==0 or m==2) spr(223,x,y)
	if m==1 or m==2 then
		spr(223,x+w-7,y,1,1,true,false)
	end
	pal()	
end

function print_cen(str,y,c,x,w)
	x = x or 0
	w = w or 128
	str = tostr(str)
	print(
		str,
		(w-#str*4+2)/2-1+x,
		y,
		c
	)
end

