-- hand
hand = card_cont:new(14,92,100)

function hand.clear()
	clear(hand.cards)
end

function hand.clear_sel()
	local cards = {}
	for c in all(hand.sel) do
		add(cards,hand.cards[c+1])
	end
	clear(hand.sel)
	for c in all(cards) do
		hand:del(c)
	end
end

function hand.sort_r()
	qsort(
		hand.cards,
		function (a,b)
			if a.rank == b.rank then
				return a.suit > b.suit
			end
			return a.rank > b.rank
		end
	)
end

function hand.sort_s()
		qsort(
		hand.cards,
		function (a,b)
			return a.suit > b.suit
		end
	)
end

function hand.type()
	if #hand.sel==0 then
		return types:empty()
	end

	local ranks = {}
	local suits = {}
	
	for c in all(hand.sel) do
		c = hand.cards[c+1]
		add(ranks,c.rank)
		add(suits,c.suit)
	end
	
	-- get the fequency
	local r_feq = feq(ranks)
	local s_feq = feq(suits)
	local r_vals = vals(r_feq)
	
	-- flush
	local flsh = 
		have(vals(s_feq),5)
	
	-- straight or ace straight
	local stra =
		(cons(keys(r_feq)) or 
		keys(r_feq) == {2,3,4,5,14})
		
	local royal = 
		keys(r_feq) == 
		{10,11,12,13,14}
		
	local hous = 
		have(r_vals,2) and
		have(r_vals,3)
		
	if 
		have(r_vals,5) and
		flsh
	then
		return types:flsh_five()
	end
	
	if flsh and hous then
		return types:flsh_hous()
	end
	
	if have(r_vals,5) then
		return types:five()
	end
		
	if royal and flsh then
		return types:royal_flsh()
	end
	
	if (stra and flsh) then
		return types:stra_flsh()
	end
	if (have(r_vals,4)) then
		return types:quad()
	end
	if (hous) return types:hous()
	if (flsh) return types:flsh()
	if (stra) return types:stra()
	if have(r_vals,3) then
		return types:trip()
	end
	if count(r_vals,2) == 2 then
	 return types:two_pair()
	end
	if have(r_vals,2) then
		return types:pair()
	end
	return types:high()
end

