-- balalow
-- by mouthless

gfx=""

anim = {}
show_con = true
state = 1

base_chip={
	[0]=100,300,800,2000,5000,
	11000,20000,35000,50000
}
cur_blind = 0

o_con = "play"
x_con = "discard"

function _init()
	deck.init()
	deck.shuffle()
	deck.draw(8)
	hand.sort_r()
	
	cur.cont = hand
	
	jokers = card_cont:new(2,3,85)
	tarots = card_cont:new(92,3,34)
	
	target = 600
	score = 0
	chip = 0
	mult = 0
	h_type = types:empty()
	scoring=false
	
	play = 4
	disc = 3
	
	ante = 1
	
	money = 0

	for x=0,4 do
		jokers:add(
			joker:new(x)
		)
	end
	
	for x=2,3 do
		tarots:add(card:new(x,1))
	end
end

function _update()
	cls(1)
	input()
	
	qsort(hand.sel)
	
	for a in all(anim) do
		if 
			costatus(a) != "dead" 
		then
			assert(coresume(a))
		else
			del(anim,a)
		end
	end
end

function _draw()
	jokers:draw()
	tarots:draw()
	
	head_ui()
	if (state==0) blind_ui()
	if (state==1) round_ui()
	if (state==2) return
end

-- ui
function head_ui()
	
	local pre_x = 
		59-(#tostr(chip))*7
	
	rrect(23,30,36,12,0) -- chip
	spr(143,60,30) -- x
	rrect(68,30,36,8,1) -- mult
	
	-- chip
	num_print(
		chip,
		pre_x,30
	)
	-- mult
	num_print(
		mult,
		68,30
	)
	
	-- hand
	if not scoring then
		rrect(23,40,81,5,2)
		print_cen(h_type.name,41,7)
	end	
	
	-- hand
	spr(175,0,30)
	num_print(play,7,30,12)
	-- disc
	spr(191,0,40)
	num_print(disc,7,40,8)
	
	-- ante
	num_print(ante,106,30,9)
	spr(159,113,30)
	num_print(8,120,30,9)
	
	-- money
	spr(207,106,39)
	print(money,115,41,10)
	
	if show_con then
		local s = 
			"🅾️ "..o_con..
			" ❎ "..x_con
		print_cen(s,50,6)
	end
end

function round_ui()
	-- score
	pre_x = 
		59-(#tostr(score))*7
	
	rrect(23,119,36,5,0)
	rrect(68,119,36,5,1)
	
	num_print(score,pre_x,119,6)
	spr(159,60,119)
	num_print(target,68,119,9)
	hand:draw()
end

function blind_ui()
	for i=0,2 do
		local x=i*39+10
		rect(
			x,57,
			x+30,130,
			i==cur_blind and cur.cont==hand
				and (t()*10)%7+8
				or 0
		)
		local f = 1
		if (i==1) f=1.5
		if (i==2) f=2
		rectfill(x+1,58,x+29,130,5)
		print_cen(
			"target",
			86,7,x+1,30
		)
		print_cen(
			base_chip[ante]*f,
			93,7,x+1,30
		)
	end
	
	-- small blind icon
	circfill(25,71,12,12)
	print("small\nblind",16,66,7)
	-- big blind icon
	circfill(64,71,12,9)
	print(" big\nblind",55,66,1)
	
	circfill(103,71,12,2)
end

-- input
function input()
	if state==1 then
		round_input()
	end
	
	if
		state != 1 and
		cur.cont == hand
	then
		goto switch
	end
	
	if cur.ind != -1 do
		if (btnp(➡️)) then 
			cur.ind += 1
			sfx(1)
		end
		if (btnp(⬅️)) then
			cur.ind -= 1
			sfx(1)
		end
		
		local t = cur.cont==jokers 
					and tarots 
					or jokers
		
		if 
			cur.cont==jokers or 
			cur.cont==tarots 
		then
			if cur.ind>=#cur.cont.cards then
				cur.cont=t
				cur.ind=0
			elseif cur.ind<0 then
				cur.cont=t
				cur.ind=#cur.cont.cards-1
			end
		end
		
		cur.ind%=#cur.cont.cards
	end
	
	if 
		btnp(⬇️) and 
		cur.ind!=-1
	then
		if
			not have(hand.sel,cur.ind)
			and #hand.sel==5
		then
			sfx(0)
			goto out
		end
		sfx(2)
		
		if have(hand.sel,cur.ind) then
			hand:unselect(cur.ind)
		else
			hand:select(cur.ind)
		end
	
		h_type = hand.type()
		chip = h_type.chip
		mult = h_type.mult
		
		goto out
	end
	
	::switch::
	if btnp(⬆️) then
		if cur.cont!=hand then
			cur.cont=hand
			o_con="play"
			x_con="discard"
		else
			cur.cont=jokers
			o_con="use"
			x_con="sell"
		end
		cur.ind=0
	end
	
	::out::
end

function round_input()
	if btnp(❎) then
		if 
			#hand.sel < 1 or 
			disc <= 0
		then
			sfx(0)
			goto out
		end
		add(anim,cocreate(hand.disc))
		
		h_type = types:empty()
		chip = h_type.chip
		mult = h_type.mult
	end
	
	if btnp(🅾️) then
		if #hand.sel < 1 then
			sfx(0)
			goto out
		end
		add(anim,cocreate(hand.score))
	end
	
	::out::
end
