-- 1 right
-- 2 both
function rrect(x,y,w,c,m)
	local l = (m==0 or m==2) and 8 or 0
	local r = (m==1 or m==2) and 8 or 0
	rectfill(x+l,y,x+w-r,y+7,c)
	
	pal(7,c)
	if (m==0 or m==2) spr(223,x,y)
	if m==1 or m==2 then
		spr(223,x+w-7,y,1,1,true,false)
	end
	pal()	
end

function print_cen(str,y,c,x,w)
	x = x or 0
	w = w or 128
	str = tostr(str)
	print(
		str,
		(w-#str*4+2)/2-1+x,
		y,
		c
	)
end

function swapn(t,i) 
	local i1=i%(#t+1)
	local i2=((i)%#t)+1
	local tmp=t[i1]

	t[i1]=t[i2]
	t[i2]=tmp
end

