-- card container

cur = {
	-- selected container
	cont={},
	-- index in container
	ind=0
}

card_cont = {}
card_cont.__index = card_cont

-- create a new hand container
-- x: top left x
-- y: top left y
-- w: width
function	card_cont:new(x,y,w)
	return setmetatable(
		{
			x=x,y=y,w=w,
			cards={},
			sel={},
			-- overlap amount
			over=0
		},
		card_cont
	)
end

function card_cont:add(c)
	add(self.cards,c)
	self.over=
		(self.w-c_w)/(#self.cards-1)
end
function card_cont:del(c)
	del(self.cards,c)
	self.over=
		(self.w-c_w)/(#self.cards-1)
end

function card_cont:select(i)
	add(self.sel,i)
end

function card_cont:unselect(i)
	del(self.sel,i)
end

function card_cont:unsel_all()
	for i in all(self.sel) do
		del(self.sel,i)
	end
end

function card_cont:have_cur()
	return cur.cont == self
end

function card_cont:draw()
	-- draw based card
	for i=1,#self.cards do
		-- curr card
		local c = self.cards[i]
		-- t value for trig
		local t = i/#self.cards-t()
		-- x offset
		local x = 
			(i-1)*self.over+self.x-cos(t)
		-- y offset
		local y = self.y+sin(t)
		
		-- if selected or cursor
		-- defer render
		if not
			(self:have_cur() and 
			i==cur.ind+1
			or
			have(self.sel,i-1))
		then
			c:draw(x,y)
		end
	end
	
	-- draw selected card
	for i,c in pairs(self.sel) do
		local t = i/#self.cards-t()
		if not
			(self:have_cur() 
	 	and c==cur.ind)
		then
			self.cards[c+1]:draw(
				c*self.over+self.x-cos(t),
				self.y-5+sin(t)
			)
		end
	end
	
	-- draw cursor card
	if self:have_cur() 
		and cur.ind!=-1 
	then
		local x=cur.ind*self.over+
			self.x-cos(t)
		local y=self.y+sin(t)-
			(have(self.sel,cur.ind)
			and 5
			or 0)
		-- get the cursor card
		self.cards[cur.ind+1]:draw(
			x,y
		)
		rect(
			x,y,
			x+c_w,y+c_h,
			(t()*10)%7+8
		)
	end
end


