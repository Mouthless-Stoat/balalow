-- hand
hand = card_cont:new(14,92,100)

function hand.clear()
	clear(hand.cards)
end

function hand.clear_sel()
	local cards = {}
	for c in all(hand.sel) do
		add(cards,hand.cards[c+1])
	end
	clear(hand.sel)
	for c in all(cards) do
		hand:del(c)
	end
end

function hand.sort_r()
	qsort(
		hand.cards,
		function (a,b)
			if a.rank == b.rank then
				return a.suit > b.suit
			end
			return a.rank > b.rank
		end
	)
end

function hand.sort_s()
		qsort(
		hand.cards,
		function (a,b)
			return a.suit > b.suit
		end
	)
end

function hand.type()
	if #hand.sel==0 then
		return {types:empty()}
	end

	local ranks = {}
	local suits = {}
	
	for c in all(hand.sel) do
		c = hand.cards[c+1]
		add(ranks,c.rank)
		add(suits,c.suit)
	end
	
	-- get the fequency
	local r_feq = feq(ranks)
	local s_feq = feq(suits)
	local r_vals = vals(r_feq)
	
	-- flush
	local flsh = 
		have(vals(s_feq),5)
	
	-- straight or ace straight
	local k = keys(r_feq)
	local stra =
		cons(k) or ace_stra(k)
		
	local royal = 
		keys(r_feq) == 
		{10,11,12,13,14}
		
	local hous = 
		have(r_vals,2) and
		have(r_vals,3)
	
	local ts = {}
	
	if 
		have(r_vals,5) and
		flsh
	then
		add(ts,types:flsh_five())
	end
	if flsh and hous then
		add(ts,types:flsh_hous())
	end
	if have(r_vals,5) then
		add(ts,types:five())
	end
	if royal and flsh then
		add(ts,types:royal_flsh())
	end
	if stra and flsh then
		add(ts,types:stra_flsh())
	end
	if have(r_vals,4) then
		add(ts,types:quad())
	end
	if hous then
		add(ts,types:hous())
	end
	if flsh then
		add(ts,types:flsh())
	end
	if stra then
		add(ts,types:stra())
	end
	if have(r_vals,3) or have(ts,types:quad()) then
		add(ts,types:trip())
	end
	if count(r_vals,2) == 2 then
	 add(ts,types:two_pair())
	end
	if have(r_vals,2) or have(ts,types:trip()) then
		add(ts,types:pair())
	end
	add(ts,types:high())
	
	return ts
end

function hand.scored()
	local t = hand.type()[1].id
	
	local ranks = {}
	local suits = {}
	
	for c in all(hand.sel) do
		c = hand.cards[c+1]
		add(ranks,c.rank)
		add(suits,c.suit)
	end
	
	-- get the fequency
	local r_feq = feq(ranks)
	local s_feq = feq(suits)
	local r_vals = vals(r_feq)
	
	if t==1 then
		for c in all(hand.sel) do
			
			if(c.rank==max(ranks)) return {c}
		end
	elseif t==2 or t==3 then
		local o={}
		for c in all(hand.sel) do
			c = hand.cards[c+1]
			if r_req[c.rank]==2 then
				add(o,c)
			end
		end
		return o
	elseif t==4 then
		local o={}
		for c in all(hand.sel) do
			c = hand.cards[c+1]
			if r_req[c.rank]==3 then
				add(o,c)
			end
		end
		return o
	elseif 
		t==5 or t==6 or t==7 or
		t==9 or t==10 or t==11 or
		t==12 or t==13 
	then
		return hand.sel
	elseif t==8 then
		local o={}
		for c in all(hand.sel) do
			c = hand.cards[c+1]
			if r_req[c.rank]==4 then
				add(o,c)
			end
		end
		return o
	end
end

-- round
function hand.score()
	show_con=false
	cur.ind=-1
	play-=1
	
	-- collect shit
	local cards = {}
	qsort(hand.sel)
	for c in all(hand.sel) do
		add(cards,hand.cards[c+1])
	end
	hand.clear_sel()
	
	-- draw scoring cards
	local over =
		(100-c_w)/(#cards-1)
	
	for i=1,#cards do
		local c=cards[i]
		chip+=c.value
		sfx(3)
		pause_f(5,function(t)
		
		local x = (i-1)*over+14
		local y = 60

		for j=1,#cards do
			if i==j then goto cont end
			local c = cards[j]
			c:draw(
				(j-1)*over+14,
				y+5
			)
			::cont::
		end
		c:score(x,y,i,t)
		
		for j in all(jokers.cards) do
			if (j.j.act_t!=1) goto cont
			j.acting=true
			j.j.effect(c)
			j.acting=false
			::cont::
		end
		end)
	end
	
	pause(10)
	for j in all(jokers.cards) do
		if (j.j.act_t!=3) goto cont
		j.acting=true
		j.j.effect(cards)
		pause(3)
		j.acting=false
		::cont::
	end
	
	local t = chip*mult
	function p_t()
		rrect(23,40,81,5,2)
		num_print(
			t,
			(67-#tostr(t)*7)/2+30,
			40
		)
	end
	
	local s = score+t
	scoring=true
	pause_f(10,p_t)
	local st = t\20
	while t > 0 do
		score+=st
		pause_f(2,p_t)
		t-=st
	end
	scoring=false
	score=s

	
	-- reset everything
	h_types={types:empty()}
	chip=0
	mult=0
	cur.ind = 0
	for i=1,#cards do
		sfx(5)
		deck.draw()
		hand.sort_r()
		pause(5)
	end
	show_con=true
end

function hand.disc()
	cur.ind=-1
	disc-=1
	
	-- collect card
	local cards = {}
	qsort(hand.sel)
	for c in all(hand.sel) do
		add(cards,hand.cards[c+1])
	end
	
	clear(hand.sel)
	
	-- delete them
	for c in all(cards) do
		sfx(4)
		hand:del(c)
		pause(5)
	end
	
	-- draw
	for i=1,#cards do
		sfx(5)
		deck.draw()
		hand.sort_r()
		pause(5)
	end
	cur.ind = 0
end

