-- round
function hand.score()
	show_con=false
	cur.ind=-1
	play-=1
	
	-- collect shit
	local cards = {}
	qsort(hand.sel)
	for c in all(hand.sel) do
		add(cards,hand.cards[c+1])
	end
	hand.clear_sel()
	
	-- draw scoring cards
	local over =
		(100-c_w)/(#cards-1)
	
	for i=1,#cards do
		local c=cards[i]
		chip+=c.value
		sfx(3)
		pause_f(5,function(t)
		
		local x = (i-1)*over+14
		local y = 60

		for j=1,#cards do
			if i==j then goto cont end
			local c = cards[j]
			c:draw(
				(j-1)*over+14,
				y+5
			)
			::cont::
		end
		c:draw(x,y)
		c:score(x,y,i,t)
		end)
	end
	
	local t = chip*mult
	function p_t()
		rrect(30,40,67,5,2)
		num_print(
			t,
			(67-#tostr(t)*7)/2+30,
			40
		)
	end
	
	local s = score+t
	scoring=true
	pause_f(10,p_t)
	local st = t\20
	while t > 0 do
		score+=st
		pause_f(1,p_t)
		t-=st
	end
	scoring=false
	score=s

	
	-- reset everything
	h_type = types:high()
	chip = 5
	mult = 1
	cur.ind = 0
	for i=1,#cards do
		sfx(5)
		deck.draw()
		hand.sort_r()
		pause(5)
	end
	show_con=true
end

function hand.disc()
	cur.ind=-1
	disc-=1
	
	-- collect card
	local cards = {}
	qsort(hand.sel)
	for c in all(hand.sel) do
		add(cards,hand.cards[c+1])
	end
	
	clear(hand.sel)
	
	-- delete them
	for c in all(cards) do
		sfx(4)
		hand:del(c)
		pause(5)
	end
	
	-- draw
	for i=1,#cards do
		sfx(5)
		deck.draw()
		hand.sort_r()
		pause(5)
	end
	cur.ind = 0
end

