-- card
-- card can have rank and suit
-- ace are store as 14
-- suit are 1,2,3,4 follow shcd
c_w = 16
c_h = 24

card = {}
card.__index = card
function card:__tostring()
	local r = ""
	
	if self.rank == 14 then
	 r = "a"
	elseif self.rank > 11 then
		local t = self.rank-10
		r = sub("jqk",t,t)
	else
		r = tostr(self.rank)
	end
	return 
	sub("shcd",self.suit,self.suit)
		..r
end

function card:new(rank,suit,j)
	local value = rank
	if rank == 14 then
		value = 11
	elseif rank >= 11 then
		value = 10
	end
	local c = {
		joker=j or false,
		rank=rank,
		value=value,
		suit=suit
	}
	return setmetatable(c,card)
end

function card:draw(x,y)
	rect(x,y,x+c_w,y+c_h,13)
	rectfill(
		x+1,y+1,
		x+c_w-1,y+c_h-1,
		7
	)
	pal(7,({12,8,11,9})[self.suit])
	x = x+(c_w-8)/2
	spr(self.rank,x+1,y+2)
	spr(16*self.suit-1,x+1,y+14)
	pal()
end

function card:score(x,y,i,t)
	srand(i)
	local sqr = 79+16*flr(rnd(4))
	local str = 
		"+"..tostr(self.value)
	spr(
		sqr,
		x+4,
		y-8+cos(t/10),
		1,1
	)
	print(
		str,
		x+(16-#str*5)/4+2,
		y-8
	)
end

