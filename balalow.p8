j_d={}
j_d.__index=j_d

-- rarity pool 0,1,2
c_js={}
u_js={}
r_js={}

cur_j=0

function j_d:new(
	name,
	price,
	rarity,
	-- activation types
	-- 0 when played
	-- 1 card
	-- 2 held
	-- 3 last
	act_t,
	effect,
	pal_swap
)
	local j = setmetatable(
		{
			name=name,
			price=price,
			s_off=cur_j*345+1,
			effect=effect,
			rarity=rarity,
			act_t=act_t,
			pal_swap=pal_swap
		},
		j_d
	)
	if (not pal_swap) cur_j+=1
		
	if rarity==0 then
		add(c_js,j)
	elseif rarity==1 then
		add(u_js,j)
	elseif rarity==2 then
		add(r_js,j)
	end
	return j
end

joker={}
joker.__index=joker

function joker:new(j)
	return setmetatable(
		{
			j=j,
			acting=acting
		},
		joker
	)
end

function joker:draw(x,y)
	rect(
		x,y,
		x+c_w,y+c_h,
		self.acting and 8 or 13
	)
	if self.j.pal_swap then
		for c,s in pairs(self.j.pal_swap) do
			if (c==-1) goto cont
			pal(c,s)
			::cont::
		end
		sspr(0,8,15,23,x+1,y+1)
		pal()
		return
	end
	local g = split(sub(
		gfx,self.j.s_off,
		self.j.s_off+344
	),1)
	for i,px in pairs(g) do
		i-=1
		local c_x = i%15+1
		local c_y = i\15+1
		local c = tonum(px,0x1)
		if c!=0 then
			pset(x+c_x,y+c_y,c)
		end
	end
end

function sin_j(s) 
	return function (c)
		if c.suit==s then
			mult+=3
		end
	end
end

hand_i = 0
function hand_j(n,m)
	hand_i %= 5
	hand_i += 1
	local l = {
		types:pair(),
		types:trip(),
		types:two_pair(),
		types:stra(),
		types:flsh()
		}
	local h = l[hand_i]
	if m then
		return function()
			if have(hand:type(),h) 
			then
				mult+=n
			end
		end
	else
		return function()
			if have(hand:type(),h) 
			then
				chip+=n
			end
		end
	end
end

