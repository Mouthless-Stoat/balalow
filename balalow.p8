function input()
	if state==1 then
		round_input()
	end
	
	if
		state != 1 and
		cur.cont == hand
	then
		goto switch
	end
	
	if cur.ind != -1 do
		if (btnp(➡️)) then
			o_last_t = 0
			
			if move_card then
				cur.cont.sel[1]+=1
				cur.cont.sel[1]%=#cur.cont.cards
				swapn(
					cur.cont.cards,
					cur.ind+1
				)
			end
			cur.ind += 1
			sfx(1)
		end
		if (btnp(⬅️)) then
			o_last_t = 0
			if move_card then
				cur.cont.sel[1]-=1
				cur.cont.sel[1]%=#cur.cont.cards
				swapn(
					cur.cont.cards,
					((cur.ind-1)%#cur.cont.cards)+1
				)
			end
			cur.ind -= 1
			sfx(1)
		end
		
		if not move_card then
			local t = cur.cont==jokers 
						and tarots 
						or jokers
			if 
				cur.cont==jokers or 
				cur.cont==tarots 
			then
				if cur.ind>=#cur.cont.cards then
					cur.cont=t
					cur.ind=0
				elseif cur.ind<0 then
					cur.cont=t
					cur.ind=#cur.cont.cards-1
				end
			end
		end
		
		cur.ind%=#cur.cont.cards
	end
	if btnp(⬇️) then
		if move_card then
			move_card=false
			cur.cont:unsel_all()
			goto out
		end
		
		if t()*30 - o_last_t <= 8 then
			cur.cont:unsel_all()
			cur.cont:select(cur.ind)
			move_card=true
			goto out
		end
		o_last_t = t()*30
		
		if cur.cont!=hand then
			goto out
		end
		
		if
			not have(hand.sel,cur.ind)
			and #hand.sel==5
		then
			sfx(0)
			goto out
		end
		sfx(2)
		
		if have(hand.sel,cur.ind) then
			hand:unselect(cur.ind)
		else
			hand:select(cur.ind)
		end
	
		h_types = hand.type()
		chip = h_types[1].chip
		mult = h_types[1].mult
		
		goto out
	end
	
	::switch::
	if btnp(⬆️) then
		if move_card then
			move_card=false
			cur.cont:unsel_all()
		end
		if cur.cont!=hand then
			cur.cont=hand
			o_con="play"
			x_con="discard"
		else
			cur.cont=jokers
			o_con="use"
			x_con="sell"
		end
		cur.ind=0
	end
	
	::out::
end

function round_input()
	if btnp(❎) then
		if 
			#hand.sel < 1 or 
			disc <= 0 or move_card
		then
			sfx(0)
			goto out
		end
		add(anim,cocreate(hand.disc))
		
		h_types = {types:empty()}
		chip = h_types[1].chip
		mult = h_types[1].mult
	end
	
	if btnp(🅾️) then
		if #hand.sel < 1 or move_card then
			sfx(0)
			goto out
		end
		add(anim,cocreate(hand.score))
	end
	
	::out::
end
